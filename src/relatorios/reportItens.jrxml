	<parameter name="qtdItem" class="java.lang.Integer"/>
	<queryString>
		<![CDATA[SELECT
     produto.`id` AS Id,
     produto.`nome` AS Nome,
     produto.`fabricante` AS Fabricante,
     produto.`preco` AS Preco,
     estoque.`qtd` AS Quantidade
FROM
     `produto` produto
INNER JOIN
     `estoque` estoque
ON estoque.`id` = produto.`id`

WHERE estoque.`qtd` >= $P{qtdItem}]]>
